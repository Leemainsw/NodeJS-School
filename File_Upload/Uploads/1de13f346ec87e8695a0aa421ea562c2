DROP USER "exDB3214";
-- USER SQL
CREATE USER "EXDB3214" IDENTIFIED BY "1234"  
DEFAULT TABLESPACE "USERS"
TEMPORARY TABLESPACE "TEMP";

-- QUOTAS
ALTER USER "EXDB3214" QUOTA 10M ON "USERS";

-- ROLES
GRANT "DBA" TO "EXDB3214" ;
GRANT "CONNECT" TO "EXDB3214" ;
GRANT "RESOURCE" TO "EXDB3214" ;

-- SYSTEM PRIVILEGES

